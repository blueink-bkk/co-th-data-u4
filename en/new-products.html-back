<script>
   $(".change_language").on("click", function(){
   	var current_url = window.location.href;
   	var lng = $(this).attr("alt");
   	var changed_url;
   	if(lng == 'th') {
   		changed_url = current_url.replace("/en/", "/th/");
   	} else {
   		changed_url = current_url.replace("/th/", "/en/");
   	}
   	
   	//alert(changed_url);
   	window.location.replace(changed_url);
   });
</script>
